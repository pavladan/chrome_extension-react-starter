  },
  "homepage": "https://github.com/pavladan/chrome_extension-react-starter#readme",
  "devDependencies": {
    "cross-env": "^7.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.0.0",
    "html-webpack-plugin": "^4.0.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "node-sass": "^4.13.1",
